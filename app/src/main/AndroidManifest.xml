<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.debugthugs">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DebugThugs">
        <activity
            android:name=".RockPaperScissors"
            android:label="@string/title_activity_rock_paper_scissors"
            android:theme="@style/Theme.DebugThugs.NoActionBar"></activity>
        <activity
            android:name=".CoinFlip"
            android:label="@string/title_activity_coin_flip"
            android:theme="@style/Theme.DebugThugs.NoActionBar" />
        <activity
            android:name=".TicTacToe"
            android:label="@string/title_activity_tic_tac_toe"
            android:theme="@style/Theme.DebugThugs.NoActionBar" />
        <activity
            android:name=".ChooseSpell"
            android:label="@string/title_activity_choose_spell"
            android:theme="@style/Theme.DebugThugs.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>